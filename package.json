{
  "name": "sar-client",
  "description": "SAR-Client App",
  "author": {
    "name": "Sea Watch",
    "email": "nic@sea-watch.org"
  },
  "version": "0.0.1-beta.22",
  "license": "MIT",
  "main": "dist/index.js",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "cp src/config/example_config.js src/config/config.js",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "run-s ng:test",
    "fmt": "tsfmt -r --baseDir src",
    "pretest": "run-s fmt",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "preng:build": "run-s fmt generate:all",
    "postng:build": "run-s maptiles:clean maptiles:extract",
    "preng:build:noprogress": "run-s generate:all",
    "postng:build:noprogress": "run-s maptiles:clean maptiles:extract",
    "ng:build": "ng build",
    "ng:build:watch": "ng build --watch",
    "ng:build:noprogress": "ng build --progress=false",
    "ng:test": "ng test --single-run",
    "ng:test:watch": "ng test --watch",
    "electron": "run-s ng:build electron:start",
    "electron:start": "electron .",
    "pack:mac": "run-s ng:build:noprogress pack:build:mac",
    "pack:win": "run-s ng:build:noprogress pack:build:win",
    "pack:linux": "run-s ng:build:noprogress pack:build:linux",
    "pack:build:mac": "build -c electron-builder.yml --mac --x64",
    "pack:build:win": "build -c electron-builder.yml --win --ia32",
    "pack:build:linux": "build -c electron-builder.yml --linux --x64",
    "maptiles:clean": "rimraf dist/MapQuest",
    "maptiles:extract": "extract-zip data/map-tiles.zip dist/",
    "generate:all": "run-s generate:version",
    "generate:version": "node script/generate-version.js"
  },
  "private": true,
  "dependencies": {
    "class-transformer": "^0.1.8",
    "cloudant-backup": "0.0.2",
    "cloudant-user": "^2.0.0",
    "couchbackup": "^1.5.0",
    "electron-in-page-search": "^1.2.3",
    "electron-settings": "^3.0.14",
    "reflect-metadata": "^0.1.10",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-beta.32.3",
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.42",
    "angular2-openlayers": "^0.4.5",
    "cloudant-user": "^2.0.0",
    "codelyzer": "~2.0.0-beta.1",
    "commander": "^2.9.0",
    "core-js": "^2.4.1",
    "cradle": "^0.7.1",
    "dexie": "2.0.0-rc.1",
    "dirty-json": "^0.4.0",
    "electron": "^1.4.15",
    "electron-builder": "^16.3.0",
    "extract-zip": "1.6.0",
    "html-pdf": "^2.1.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "mail-listener2": "^0.3.1",
    "mustache": "^2.3.0",
    "nano": "^6.2.0",
    "nodemailer": "^4.0.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm-run-all": "^4.0.1",
    "pouchdb": "^6.1.1",
    "pouchdb-find": "^6.1.1",
    "protractor": "~5.1.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.0.1",
    "semver": "^5.3.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "^2.1.0",
    "tslint": "^4.3.0",
    "typescript": "~2.2.1",
    "typescript-formatter": "^5.1.1",
    "uuid": "^3.1.0",
    "zone.js": "^0.7.4"
  }
}
